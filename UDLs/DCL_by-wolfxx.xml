            <WordsStyle name="NUMBERS" fgColor="FF0000" bgColor="FFFFFF" fontName="Consolas" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="0000FF" bgColor="FFFFFF" fontName="Consolas" fontStyle="1" fontSize="9" nesting="0" />
