            <WordsStyle name="OPERATORS" fgColor="000080" bgColor="C0C0C0" fontName="Courier" fontStyle="0" fontSize="9" nesting="0" />
            <WordsStyle name="FOLDER IN CODE1" fgColor="000000" bgColor="C0C0C0" fontName="Courier" fontStyle="1" fontSize="9" nesting="0" />
            <WordsStyle name="FOLDER IN CODE2" fgColor="00FF40" bgColor="C0C0C0" fontName="Courier" fontStyle="1" fontSize="9" nesting="0" />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
